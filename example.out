; ModuleID = 'compyle'
source_filename = "compyle"

declare i32 @printf(i8*, ...)

define i32 @main() {
entry:
  %x = alloca i32, align 4
  store i32 3, i32* %x, align 4
  br label %while

while:                                            ; preds = %while_end, %entry
  br i1 true, label %while_body, label %while_end3

while_body:                                       ; preds = %while
  %0 = load i32, i32* %x, align 4
  %tmp = add i32 %0, 1
  store i32 %tmp, i32* %x, align 4
  br label %while1

while1:                                           ; preds = %while_body
  br i1 true, label %while_body2, label %while_end

while_body2:                                      ; preds = %while1
  ret i32 1

while_end:                                        ; preds = %while1
  br label %while

while_end3:                                       ; preds = %while
  ret i32 0
}
