; ModuleID = 'compyle'
source_filename = "compyle"

@x = global [25 x i8] c"Hello World from global!\00"
@0 = private unnamed_addr constant [11 x i8] c"hi locally\00", align 1

declare i32 @printf(i8*, ...)

define i32 @main() {
entry:
  %y = alloca i8*, align 8
  store i8* getelementptr inbounds ([11 x i8], [11 x i8]* @0, i32 0, i32 0), i8** %y, align 8
  %printf = call i32 (i8*, ...) @printf(i8* getelementptr inbounds ([25 x i8], [25 x i8]* @x, i32 0, i32 0))
  %y1 = load i8*, i8** %y, align 8
  %printf2 = call i32 (i8*, ...) @printf(i8* %y1)
  ret i32 0
}
