def tupes() -> int:
    x: tuple = (True, 5)
    y: bool = True
    z = 7
    while y == True and z > 3:
        if z < 5 :
            break
        z -= 1
    return z

def main() -> int:
    q: int = tupes()
    r: int = q + 5
    print(r)
    m: string = "Hello World"
    print(m)
    return 0
