"""
A test to showcase different features of ComPyle
"""
def tupes() -> int:
    x: tuple = (True, 5)
    y: bool = True
    z = 5
    if 6 not in x:
        print("yay")
    while y == True and z > 3:
        if 5 in x:
            break
        
        z -= 1
    return z

def main() -> int:
    q: int = tupes()
    r: int = q + 5
    print(r)
    m: string = "Hello World"
    print(m)
    return 0
